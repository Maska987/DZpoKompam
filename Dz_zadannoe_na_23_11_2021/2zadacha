#include <iostream>
#include <string>

using namespace std;

int main() {
	setlocale(LC_ALL, "Russian");
  char sum[80];

	gets_s(sum);
	int b = 0;
	string h;
	for (int i = 0; i < strlen(sum); i++){

		if (sum[i] != '+'){
			h += sum[i];
		}
		else{
			b += stoi(h);
			h = "";
		}
	}
	b += stoi(h);
	cout << b;
